{
  "name": "basegl",
  "version": "0.1.8",
  "description": "A blazing fast WebGL drawing API. Currently supports two-dimensional operations, including SDF shapes, animation, mouse handling and SDF text (both online atlas baking as well as rendering).",
  "homepage": "https://luna.github.io/basegl/",
  "author": "Wojciech Danilo <wojciech.danilo@gmail.com>",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "build": "npx coffee --compile --output lib/ src/ && cp package.json lib/package.json",
    "watch": "npx coffee --watch --compile --output lib/ src/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luna/basegl.git"
  },
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "webgl",
    "2d",
    "graphics"
  ],
  "bugs": {
    "url": "https://github.com/luna/basegl/issues"
  },
  "devDependencies": {
    "coffee-loader": "^0.9.0",
    "coffeescript": "next",
    "gl-matrix": "^2.4.0",
    "webpack": "^3.9.1"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "stats.js": "^0.17.0",
    "three": "^0.85.2",
    "three-effectcomposer-es6": "^0.0.4",
    "spectorjs": "^0.9.2",
    "glslify-loader": "^1.0.2",
    "raw-loader": "^0.5.1",
    "webpack-dev-server": "^2.9.7",
    "acorn": "^5.3.0",
    "escodegen": "^1.9.0",
    "glsl-gamma": "^2.0.0",
    "opentype.js": "^0.7.3",
    "gulp": "next",
    "gulp-coffee": "^3.0.2",
    "coffeescript": "^2.1.1",
    "gulp-transform": "^3.0.5",
    "gulp-rename": "^1.2.2",
    "gulp-plumber": "^1.2.0"
  }
}
